{
  "Redis": {
    "Configuration": "localhost,password=admin123@"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Elasticsearch" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": [
            "http://localhost:9200"
          ],
          "indexFormat": "log-demo-dev-{0:yyyy.MM.dd}",
          "autoRegisterTemplate": true,
          "typeName": "logEvent",
          "inlineFields": true
        }
      }
    ]
  },
  "ElasticApm": {
    "ServerUrls": "http://localhost:8200",
    "ServiceName": "Demo",
    "Environment": "Development"
  },
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "localhost",
        "Port": 5672,
        "Username": "publisher",
        "Password": "publisher123@",
        "VirtualHost": "demo"
      }
    },
    "EventBus": {
      "ClientName": "PublisherDev",
      "ExchangeName": "DemoDev"
    }
  },
  "Kafka": {
    "Connections": {
      "Default": {
        "BootstrapServers": "localhost:9093"
      },
      "Username": "admin",
      "Password": "admin123@"
    },
    "EventBus": {
      "GroupId": "DemoDev",
      "TopicName": "yan.demo"
    }
  }
}